{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "KOTLIN_VERSION": "1.7.20",
          "ORG_GRADLE_PROJECT_kotlinVersion": "1.7.20",
          "ORG_GRADLE_PROJECT_kotlin.suppressKotlinVersionCompatibilityCheck": "true"
        },
        "buildType": "apk",
        "prebuildCommand": "node kotlin-fix.js"
      }
    },
    "production": {
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "KOTLIN_VERSION": "1.7.20",
          "ORG_GRADLE_PROJECT_kotlinVersion": "1.7.20",
          "ORG_GRADLE_PROJECT_kotlin.suppressKotlinVersionCompatibilityCheck": "true"
        },
        "buildType": "apk",
        "prebuildCommand": "node kotlin-fix.js"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
